{
  "manifest_version": 3,
  "permissions": ["scripting"],
  "name": "Refresh Page",
  "version": "1.0",
  "description": "Refreshes the web page every 5 minutes.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}